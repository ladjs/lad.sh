    "overrides": [
      {
        "files": [
          "assets/js/*.js",
          "assets/js/**/*.js"
        ],
        "envs": [
          "browser"
        ],
        "plugins": [
          "compat",
          "@lwc/eslint-plugin-lwc",
          "no-smart-quotes"
        ],
        "rules": {
          "compat/compat": "error",
          "promise/prefer-await-to-then": "off",
          "no-smart-quotes/no-smart-quotes": "error",
          "unicorn/no-null": "off"
